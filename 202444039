#include<stdio.h>
int main()
{
    int num;
    printf("단을 입력하시오.");
    scanf("%d",&num);
    int i=1;
    while(i <=9)
    {
        printf("%d * %d = %d\n",num,i,(num*i));
        i++;
    }
    //구구단코드
        int sum =0;
   for(int i = 1; i<=10; i++)
    {
        sum +=i;
    }
      printf("1~10까지의 합 : %d",sum);
      //1~10까지의 합 코드
      



    
    return 0;
}

