#include<stdio.h>
int main()
{
    int num;
    printf("단을 입력하시오.");
    scanf("%d",&num);
    int i=1;
    while(i <=9)
    {
        printf("%d * %d = %d\n",num,i,(num*i));
        i++;
    }
    //구구단코드
        int sum =0;
   for(int i = 1; i<=10; i++)
    {
        sum +=i;
    }
      printf("1~10까지의 합 : %d",sum);
      //1~10까지의 합 코드

    int jumsu;
    char grade;

    printf("점수 입력 :");
    scanf("%d",&jumsu);

    if(jumsu >= 90)
    {
        grade = 'A';
    }
    
    else if(jumsu >= 80)
    {
        grade = 'B';
    }
    else if(jumsu >= 70)
    {
        grade = 'C';
    }
    else if(jumsu >= 60)
    {
        grade = 'D';
    }
     else
    {
        grade = 'F';
    }
    printf("당신의 등급은 : %c",grade);
//시험 등급 코드

    int x,y;
    char op;
    scanf("%d %c %d",&x,&op,&y);
    if(op == '+')
    {
        printf("%d %c %d = %d",x,op,y,x+y);
    }
    else if(op == '-')
    {
        printf("%d %c %d = %d",x,op,y,x-y);
    }
     else if(op == '*')
    {
       printf("%d %c %d = %d",x,op,y,x*y);
    }
     else if(op == '/')
    {
       printf("%d %c %d = %d",x,op,y,x/y);
    }
//if문
    switch(op)
    {
        case '+':
        printf("%d %c %d = %d",x,op,y,x+y);
        break;

        case '-':
        printf("%d %c %d = %d",x,op,y,x-y);
        break;

        case '*':
        printf("%d %c %d = %d",x,op,y,x*y);
        break;

        case '/':
        printf("%d %c %d = %d",x,op,y,x/y);
        break;
//sweak문


}
//4칙연산


      



    
    return 0;
}

