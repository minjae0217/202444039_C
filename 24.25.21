int b_s(int list[],int n,int input){
    int low=0;
    int high=n-1;
    int md,cnt=1;

    while(low<=high){
        printf("%d회 이진 탐색 : [%d %d]\n",cnt,low,high);
        md=(low+high)/2;

        if(input==list[md]){
            return md;
        }
        else if(input>list[md]){
            low=md+1;
        }
        else{
            high=md-1;
        }
        cnt++;
    }
    return-1;
}

